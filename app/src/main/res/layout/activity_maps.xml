<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsActivity"
    android:background="#E4FFFA" >

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="383dp"
        android:layout_height="572dp"
        android:layout_margin="15dp"
        tools:context="com.example.user.Problert.MapsActivity"
        tools:ignore="DuplicateIds" />

    <ImageView
        android:layout_height="132dp"
        android:layout_width="wrap_content"
        android:id="@+id/Topbar"
        android:src="@drawable/bar1"/>

    <Button
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/myLocation"
        android:text="현재 위치"
        android:onClick="onLastLocationButtonClicked"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="600dp"
        tools:ignore="OnClick" />

    <Button android:background="@drawable/pen"
        android:layout_height="83dp"
        android:layout_width="83dp"
        android:id="@+id/writepage"
        android:onClick="writebutton"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="540dp"/>

